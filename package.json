{
  "name": "datadog-apm-azure-vending-machine",
  "version": "1.0.0",
  "description": "GitHub-hosted vending machine UI for enabling Datadog APM on Azure App Services",
  "main": "index.js",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test:ci": "CI=true yarn test:coverage",
    "quality": "yarn type-check && yarn lint && yarn format:check",
    "check-peers": "npx check-peer-dependencies",
    "check-peers:solutions": "npx check-peer-dependencies --findSolutions",
    "explain-peer-requirements": "./scripts/yarn-explain-peer-requirements.sh",
    "audit-fix": "./scripts/yarn-audit-fix.sh",
    "npm-audit-fix-force": "./scripts/npm-audit-fix-force.sh"
  },
  "keywords": [
    "datadog",
    "apm",
    "azure",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-browser": "^4.0.0",
    "@azure/msal-react": "^3.0.0",
    "@hookform/resolvers": "^3.3.2",
    "@jest/transform": "30.0.4",
    "@jest/types": "30.0.1",
    "@tanstack/react-query": "^5.8.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "babel-jest": "30.0.4",
    "clsx": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.48.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-flow": "^7.24.0",
    "@babel/plugin-transform-react-jsx": "^7.25.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "5.62.1-alpha.0",
    "@typescript-eslint/parser": "5.62.1-alpha.0",
    "audit-ci": "^6.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.8",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1"
  },
  "resolutions": {
    "postcss": "^8.4.31",
    "nth-check": "^2.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
